---
import ContextMoreMenu from "./react/ContextMoreMenu";

export interface Props {
  contentType: "thread" | "note" | "space" | "dashboard" | "profile";
  contentId?: string;
}

const { contentType = "dashboard", contentId } = Astro.props;
---

<ContextMoreMenu
  contentType={contentType}
  contentId={contentId}
  client:load
/>
