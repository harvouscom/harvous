---
import ActionButtonReact from "@/components/react/ActionButton";

export interface Props {
  variant?: "default" | "Add" | "Close";
  [key: string]: any;
}

const { variant = "default", ...props } = Astro.props;
---

<ActionButtonReact
  variant={variant}
  client:load
  {...props}
>
  <slot />
</ActionButtonReact>


